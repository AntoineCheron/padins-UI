{
  "name": "padins-ui",
  "version": "0.0.2",
  "description": "Padins-ui is a flow-based programming oriented interface for designing computational science simulations",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "start": "tsc && webpack-dev-server --open --port 3000",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "webpack-dev": "set NODE_ENV=development && webpack",
    "webpack-prod": "npm run ngc && set NODE_ENV=production && webpack"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "@types/backbone": "^1.3.34",
    "@types/jointjs": "^1.0.4",
    "@types/jquery": "^2.0.46",
    "@types/underscore": "^1.8.1",
    "angular-in-memory-web-api": "~0.3.0",
    "angular-tree-component": "^3.7.3",
    "angular2-highcharts": "^0.5.5",
    "angular2-uuid": "^1.1.1",
    "bootstrap-treeview": "^1.2.0",
    "clean-webpack-plugin": "^0.1.16",
    "core-js": "^2.4.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "golden-layout": "^1.5.8",
    "html-webpack-plugin": "^2.28.0",
    "ie-shim": "^0.1.0",
    "jointjs": "^1.1.0",
    "monaco-editor": "^0.8.3",
    "ng2-monaco-editor": "0.0.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "typescript": "^2.3.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.16.4",
    "protractor": "~4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.2.1",
    "tslint": "^3.15.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "repository": {}
}
